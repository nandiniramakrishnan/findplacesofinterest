{
  "name": "findplacesofinterest",
  "version": "1.0.0",
  "description": "A web application to find nearby places of interest, using React.js, Node.js and Foursquare",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d static/js",
    "bundle": "./node_modules/browserify/bin/cmd.js static/js/locationform.js -o static/js/bundle.js",
    "test": "jest",
    "start": "npm run build && npm run bundle && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nandiniramakrishnan/findplacesofinterest.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nandiniramakrishnan/findplacesofinterest/issues"
  },
  "homepage": "https://github.com/nandiniramakrishnan/findplacesofinterest#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "request-promise": "^4.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "bluebird": "^3.5.0",
    "browserify": "^14.3.0",
    "jest": "^19.0.2",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "stealthy-require": "^1.0.0",
    "webpack": "^2.2.0"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx"
    ],
    "presets": ["env", "es2015", "react"]
  },
  "jest": {
    "moduleDirectories": ["node_modules"]
  }
}
